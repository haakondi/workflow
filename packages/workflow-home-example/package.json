{
  "name": "workflow-user-home",
  "private": true,
  "version": "1.0.6",
  "description": "User workflow home directory, generated by create-workflow-home",
  "main": "cli.js",
  "license": "MIT",
  "keywords": [
    "workflow-wm"
  ],
  "babel": {
    "presets": [
      "flow",
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start": "node cli.js"
  }
}
